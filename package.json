{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run format && next dev --turbopack",
    "build": "npm run format && next build",
    "start": "npm run format && next start",
    "lint": "next lint",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
